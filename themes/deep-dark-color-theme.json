    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#4a5068",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new"
            ],
            "settings": {
                "foreground": "#9d7cd8"
            }
        },
        {
            "scope": [
                "storage",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#9d7cd8"
            }
        },
        {
            "scope": [
                "constant",
                "constant.numeric",
                "constant.language"
            ],
            "settings": {
                "foreground": "#ff9e64"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#9ece6a"
            }
        },
        {
            "scope": [
                "variable",
                "variable.other",
                "variable.language"
            ],
            "settings": {
                "foreground": "#a9b1d6"
            }
        },
        {
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#e0af68"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#7aa2f7"
            }
        },
        {
            "scope": [
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#0db9d7"
            }
        },
        {
            "scope": [
                "entity.name.class",
                "entity.name.type.class"
            ],
            "settings": {
                "foreground": "#2ac3de"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#2ac3de"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#f7768e"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#bb9af7"
            }
        },
        {
            "scope": [
                "punctuation",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#9abdf5"
            }
        },
        {
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#89ddff"
            }
        },
        {
            "scope": [
                "text.html.markdown",
                "meta.embedded.block.markdown",
                "markup.content",
                "source.chat"
            ],
            "settings": {
                "foreground": "#505050"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#454545",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.raw",
            "settings": {
                "foreground": "#505050"
            }
        }
    ]
}